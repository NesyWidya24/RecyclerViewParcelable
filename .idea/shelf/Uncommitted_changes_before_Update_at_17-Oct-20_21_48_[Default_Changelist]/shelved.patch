+package com.kagu.dengueapp;
+
+import androidx.appcompat.app.AppCompatActivity;
+import androidx.core.content.ContextCompat;
+
+import android.content.Intent;
+import android.database.Cursor;
+import android.graphics.Color;
+import android.os.Build;
+import android.os.Bundle;
+import android.util.Log;
+import android.view.View;
+import android.widget.Button;
+import android.widget.ImageView;
+import android.widget.ListView;
+import android.widget.TextView;
+import android.widget.Toast;
+
+import java.io.IOException;
+import java.util.Random;
+
+public class QuestionActivity extends AppCompatActivity {
+    private String arrayData[][];
+    private Button btnA, btnB;
+    private int next, prev;
+    private ImageView back, repeat;
+    private TextView btnNext, questionScroll, idquestion, btnPrev, titleQuestion;
+
+    private DatabaseOpenHelper dbClass;
+    private Cursor availableData;
+
+    private String a1 = "a1", a2 = "a2", a3 = "a3", a4 = "a4";
+    private String question_id, question, answerA, answerB, bbtA, bbtB, bbtListA, bbtListB;
+    private float ba1, ba2, ba3, ba4;
+
+    Random questionRandom;
+    Boolean isAnswer = false;
+
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        setContentView(R.layout.activity_question);
+        //inisialiasasi
+        questionScroll = findViewById(R.id.scrollText);
+        btnNext = findViewById(R.id.next);
+        btnPrev = findViewById(R.id.prev);
+        btnA = findViewById(R.id.answer_a);
+        btnB = findViewById(R.id.answer_b);
+        titleQuestion = findViewById(R.id.soal);
+        idquestion = findViewById(R.id.question_id);
+        repeat = findViewById(R.id.refresh);
+        back = findViewById(R.id.back);
+
+        //take db
+        dbClass = new DatabaseOpenHelper(this);
+        dbClass.openDB();
+        try {
+            dbClass.SetDatabase();
+        } catch (IOException e) {
+            e.printStackTrace();
+        }
+
+        //random question
+        questionRandom = new Random();
+        showQuestion(idquestion.getText().toString());
+        isAnswered(Integer.parseInt(idquestion.getText().toString()));
+
+        //back direction
+        back.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View view) {
+                finish();
+                startActivity(new Intent(QuestionActivity.this, MainActivity.class));
+            }
+        });
+
+        //set next prev question
+        btnNext.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View view) {
+                if (isAnswer) {
+                    next = Integer.parseInt(idquestion.getText().toString() + 1);
+                    if (next <= 14) {
+                        isAnswer = false;
+                        showQuestion(String.valueOf(next));
+                        isAnswered(next);
+                        idquestion.setText("" + next);
+                        if (next == 14) {
+                            btnNext.setText(R.string.doneQuestion);
+                        }
+                    } else {
+                        finish();
+                        startActivity(new Intent(QuestionActivity.this, MyResultActivity.class));
+                    }
+                } else {
+                    Toast.makeText(QuestionActivity.this, getString(R.string.notifNoAnswer), Toast.LENGTH_SHORT).show();
+                }
+            }
+        });
+
+        btnPrev.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View view) {
+                prev = Integer.parseInt(idquestion.getText().toString()) - 1;
+                if (prev >= 1) {
+                    showQuestion(String.valueOf(prev));
+                    isAnswered(prev);
+                    idquestion.setText("" + prev);
+                }
+            }
+        });
+
+        //repeat question
+        repeat.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View view) {
+                Toast.makeText(QuestionActivity.this, "Repeat Question", Toast.LENGTH_SHORT).show();
+                clearData();
+                Intent intent = getIntent();
+                finish();
+                startActivity(intent);
+            }
+        });
+
+        btnA.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View view) {
+                selectedBtn(btnA);
+                unselectedBtn(btnB);
+
+                bbtInit();
+            }
+        });
+    }
+
+    private void bbtInit() {
+    }
+
+    private void clearData() {
+        dbClass.truncateTabel();
+        try {
+            dbClass.SetDatabase();
+        }catch (IOException e){
+            e.printStackTrace();
+        }
+    }
+
+    private void isAnswered(int setPrevNext) {
+        availableData = dbClass.ShowDataAltByID(setPrevNext);
+        if (availableData.getCount() != 0) {
+            while (availableData.moveToNext()) {
+                String answerColumn = availableData.getString(10);
+                String answer = answerColumn.trim().toLowerCase().replace(" ", "");
+                String btn_A = btnA.getText().toString().trim().toLowerCase().replace(" ", "");
+                String btn_B = btnB.getText().toString().trim().toLowerCase().replace(" ", "");
+
+                if (btn_A.equals(answer)) {
+                    selectedBtn(btnA);
+                    unselectedBtn(btnB);
+                } else if (btn_B.equals(answer)) {
+                    selectedBtn(btnB);
+                    unselectedBtn(btnA);
+                }
+                Log.d("COLUMN", answer + " " + btn_A + " " + btn_B);
+            }
+        } else {
+            unselectedBtn(btnA);
+            unselectedBtn(btnB);
+        }
+    }
+
+    public void unselectedBtn(Button btn) {
+        btn.setTextColor(Color.parseColor("3D3E59"));
+        btn.setBackground(ContextCompat.getDrawable(this, R.drawable.btn_unselected));
+    }
+
+    public void selectedBtn(Button btn) {
+        isAnswer = true;
+        btn.setTextColor(Color.parseColor("3D3E59"));
+        btn.setBackground(ContextCompat.getDrawable(this, R.drawable.btn_selected));
+    }
+
+    private void showQuestion(String id) {
+        arrayData = dbClass.getMethodDataByID(id);
+
+        question_id = arrayData[0][0];
+        question = arrayData[0][1];
+        String[] split_answer = arrayData[0][3].split("#"); // memisahkan 2 data array dengan # yg bernilai kosong(tidak d anggap)
+        answerA = split_answer[0];
+        answerB = split_answer[1];
+
+        questionScroll.setText(question);
+        btnA.setText(answerA);
+        btnB.setText(answerB);
+        titleQuestion.setText(getString(R.string.idQuestion) + id);
+    }
+}
